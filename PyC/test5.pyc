require("Calc");

int main(){
	int a,b,result1;
	float result2;

	a = 6;
	b = 11;

	result1 = Calc.sum(a,b);
	output(a+"+"+b+"="+result1);
	result1 = Calc.sub(a,b);
	output(a+"-"+b+"="+result1);
	result1 = Calc.mul(a,b);
	output(a+"*"+b+"="+result1);
	result2 = Calc.div(a,b);
	output(a+"/"+b+"="+result2);
}