int main()
{
    //130 375
    int low, high, i, j, k, temp1, temp2, remainder, n, result;
    n = 1;
    result =0;

    output("Enter two numbers(intervals) :");
    low = int(input());
    high = int(input());
    output("Armstrong numbers between" + low + "an" + high + "are :");

    i = low+2;
    while(i<high){
        i = i+1;
        temp2 = i;
        temp1 = i;

        // number of digits calculation
        while (temp1 != 0){
            temp1 = temp1 / 10;
            n = n+1;
        }

        // result contains sum of nth power of its digits
        while (temp2 != 0){
            remainder = temp2 % 10;
            j = remainder;
            k=0;
            while(k<n-1){
                k = k+1;
                j = j * remainder;
            }
            result = result + j;
            temp2 = temp2 / 10;
        }

        // checks if number i is equal to the sum of nth power of its digits
        if (result == i) {
            output(i);
        }

        // resetting the values to check Armstrong number for next iteration
        n = 0;
        result = 0;
    }
}
